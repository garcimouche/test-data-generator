{
    "output": {
        "type": "file",
        "props":{
            "pathProvider":"const",
            "file": "./a.csv",
            "format": "csv"
        }
    },
    "templates": [
        {
            "id": "customer1",
            "count": 20,
            "name": "customer",
            "fields": [
                {
                    "name": "id",
                    "type": "long",
                    "provider": {
                        "name": "expr",
                        "props": {
                            "expr": "if(poId % 2 == 0) return custId++; else return custId;"
                        }
                    }
                },
                {
                    "name": "name",
                    "type": "string",
                    "provider": {
                        "name": "people-name",
                        "props":{
                            "gender":"all",
                            "type":"last"
                        }
                    }
                },
                {
                    "name": "email",
                    "type": "string",
                    "provider": {
                        "name": "email"
                    }
                } ,
                {
                    "name": "country",
                    "type": "string",
                    "provider": {
                        "name": "country"
                    }
                },
                {
                    "name": "gender",
                    "type": "int",
                    "provider": {
                        "name": "select",
                        "props":{
                            "items":"1;2;3",
                            "delimiter":";"
                        }
                    }
                },
                {
                    "name": "age",
                    "type": "int",
                    "provider": {
                        "name": "random",
                        "props":{
                            "size":"2"
                        }
                    }
                },
                {
                    "name": "quality",
                    "type": "int",
                    "provider": {
                        "name": "select",
                        "props":{
                            "items":"1,2,3,4"
                        }
                    }
                },
                {
                    "name": "poId",
                    "type": "long",
                    "provider": {
                        "name": "expr",
                        "props": {
                            "expr": "poId++"
                        }
                    }
                },
                {
                    "name": "product",
                    "type": "int",
                    "provider": {
                        "name": "random",
                        "props": {
                            "size": 999
                        }
                    }
                },
                {
                    "name": "unit_price",
                    "type": "int",
                    "provider": {
                        "name": "expr",
                        "props": {
                            "expr": "unitP = new java.util.Random().nextInt(75);unitP"
                        }
                    }
                },
                {
                    "name": "quantity",
                    "type": "int",
                    "provider": {
                        "name": "expr",
                        "props": {
                            "expr": "if(unitP >35) return 1; else return 2;"
                        }
                    }
                },
                {
                    "name": "date",
                    "type": "long",
                    "provider": {
                        "name": "time"
                    }
                }
            ]
        }
    ],
    "variables": [
        {
            "name": "custId",
            "type": "long",
            "initValue": "1"
        },
        {
            "name": "poId",
            "type": "long",
            "initValue": "1"
        },
        {
            "name": "unitP",
            "type": "int",
            "initValue": "1"
        }
    ],
    "root": [
        "customer1"
    ]
}
